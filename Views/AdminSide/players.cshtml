@model List<termProject.Models.player>
    @{
        ViewBag.Title = "players";
        Layout = "~/Views/shared/adminLayoutPage.cshtml";
    }

    <div class="col-md-9">
        <div class="jumbotron">

            <div class="container">

@for (int i = 0; i < Model.Count(); i++)
{
    <table style="display: inline-block;">
        <tr>
            <td>
                <img src="/files/@Model[i].img" width="150" height="150" />
            </td>
        </tr>
        <tr>
            <td>
                <b> @Model[i].name</b>
            </td>
        </tr>
        <tr>
            <td>
                <b>@Model[i].email</b>


            </td>
        </tr>
        <tr>
            <td>
                <b> @Model[i].password </b>
            </td>
        </tr>

    </table>



}
            </div>
        </div>
    </div>