@model List<termProject.Models.team>
@{
    ViewBag.Title = "view";
Layout = "~/Views/shared/adminLayoutPage.cshtml";
    }




        <div class="col-md-9">
            <div class="jumbotron">

                <div class="container">


                    <table >

                        <tr>
                            <td  align="center" width="600"><h3>Team Profile</h3></td>



                        </tr>
                    </table>
                    <br/>
                    <br/>


                    <form method="post" action="updateSave" enctype="multipart/form-data">
                        @for (int i = 0; i < Model.Count(); i++)
                        {

                           <table >
                               <tr>
                                   <td rowspan="6">
                                       <img src="/files/@Model[i].imgsrc" width="300" height="300" /> 
                                   </td>
                                   <td width="400" rowspan="4">
                                      <h3>Team Name : @Model[i].name </h3>
                                       <h3>Email address : @Model[i].email </h3>
                                     
                                       <h3>Password : @Model[i].password</h3>
                                   </td>
                                   </tr>
                               
                           </table>
                            
                       
                                
                                
                            <br/>

    <a href="@Url.Action("players", "AdminSide", new { id = @Model[i].Id })"><input type="button" value="See Players" /></a>


                            //string src = "delete/" + @Model[i].Id;
                        }

                    </form>













                </div>

            </div>
        </div>


    </div>
</div>
